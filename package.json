  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "npm run format"
    ]
  },
  "dependencies": {
    "@hono/node-server": "^1.13.7",
    "@rajmazumder/grabenv": "^0.1.2",
    "hono": "^4.6.10",
    "http-status-codes": "^2.3.0",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.3.20",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.15.0",
    "@types/bun": "latest",
    "@types/es6-shim": "^0.31.45",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^16.11.10",
    "@types/winston": "^2.4.4",
    "eslint": "9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "10.9.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0"
  }
}