{
  "name": "create-hono-app",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && tsc-alias",
    "lint": "eslint . --fix",
    "format": "prettier . --write",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "npm run format"
    ]
  },
  "dependencies": {
    "@hono/node-server": "^1.13.7",
    "@rajmazumder/grabenv": "^0.0.5",
    "hono": "^4.6.10"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.15.0",
    "@types/bun": "latest",
    "@types/eslint__js": "^8.42.3",
    "eslint": "9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0"
  }
}