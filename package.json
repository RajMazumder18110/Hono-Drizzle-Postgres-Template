{
  "name": "create-hono-app",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "NODE_ENV=development bun run --hot src/app/development.ts",
    "test": "jest",
    "prepare": "husky",
    "lint": "eslint . --fix",
    "format": "prettier . --write",
    "typeorm": "typeorm-ts-node-commonjs"
